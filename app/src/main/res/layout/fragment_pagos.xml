<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context=".ui.dashboard.DashboardFragment">

    <ImageView
        android:id="@+id/imageView12"
        android:layout_width="35dp"
        android:layout_height="35dp"
        android:layout_marginStart="119dp"
        android:layout_marginTop="586dp"
        android:layout_marginEnd="113dp"
        android:contentDescription="casa_inicio_imagen"
        android:src="@drawable/house"
        android:visibility="visible"
        app:layout_constraintBottom_toTopOf="@+id/txt_resumen_cuenta"
        app:layout_constraintEnd_toStartOf="@+id/imageView19"
        app:layout_constraintStart_toEndOf="@+id/pay"
        app:layout_constraintTop_toBottomOf="@+id/txt_porkybank" />

    <TextView
        android:id="@+id/txt_resumen_cuenta"
        android:layout_width="35dp"
        android:layout_height="23dp"
        android:layout_marginStart="118dp"
        android:layout_marginEnd="102dp"
        android:layout_marginBottom="12dp"
        android:text=" Inicio"
        android:textColor="#000000"
        android:textSize="12sp"
        android:visibility="visible"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toStartOf="@+id/txt_productos"
        app:layout_constraintStart_toEndOf="@+id/txt_pagos"
        app:layout_constraintTop_toBottomOf="@+id/imageView12" />

    <ImageView
        android:id="@+id/imageView19"
        android:layout_width="35dp"
        android:layout_height="35dp"
        android:layout_marginTop="661dp"
        android:layout_marginEnd="40dp"
        android:contentDescription="productos_imagen"
        android:src="@drawable/productos"
        app:layout_constraintBottom_toTopOf="@+id/txt_productos"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintTop_toTopOf="parent" />

    <TextView
        android:id="@+id/txt_productos"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginEnd="31dp"
        android:text="Productos"
        android:textColor="#000000"
        android:textSize="12sp"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/imageView19" />

    <ImageView
        android:id="@+id/pay"
        android:layout_width="35dp"
        android:layout_height="35dp"
        android:layout_marginStart="34dp"
        android:layout_marginTop="662dp"
        android:src="@drawable/pay"
        app:layout_constraintBottom_toTopOf="@+id/txt_pagos"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        android:contentDescription="TODO" />

    <TextView
        android:id="@+id/txt_pagos"
        android:layout_width="35dp"
        android:layout_height="23dp"
        android:layout_marginStart="34dp"
        android:text="Pagos"
        android:textColor="#000000"
        android:textSize="12sp"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/pay" />

    <TextView
        android:id="@+id/txt_porkybank"
        android:layout_width="170dp"
        android:layout_height="45dp"
        android:layout_marginStart="120dp"
        android:layout_marginTop="30dp"
        android:layout_marginEnd="121dp"
        android:layout_marginBottom="656dp"
        android:text="@string/app_name"
        android:textColor="@color/black"
        android:textSize="34sp"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        tools:ignore="TextSizeCheck" />
</androidx.constraintlayout.widget.ConstraintLayout>